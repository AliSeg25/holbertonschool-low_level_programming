#include "main.h"
/**
 * print_diagonal - i like diagonals
 * @n: no of times
 * Return: void
 */
void print_diagonal(int n)
{
	int l= 0,
    	int m= 0;

	if (n > 0)
{
	   for (; l < n; l++)
       	   }
	   for (m = 0; m < l; m++)
	   _putchar(' ');
	   _putchar('\\');
	   _putchar('\n');
	   }
}
	else
	{
_	putchar('\n');
	}
}
