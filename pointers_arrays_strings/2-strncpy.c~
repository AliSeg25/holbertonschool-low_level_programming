/**
 * _strncpy - cpoies n bytes of src to the dest string
 * @qrc: chaine a copie
 * @dest: chaine coller
 * @n: combien a copier
 * Return: (dest)
 */
char *_strncpy(char *dest, char *src, int n)
{
	int c = 0;

	while (i < n && src[i] != '\0')
	{
		dest[i] = src[i];
		i++;
	}

	dest[i] = '\0'

		return (dest);
}
